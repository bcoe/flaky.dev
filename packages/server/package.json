{
  "name": "flaky.dev-server",
  "version": "1.0.0",
  "description": "flaky.dev server",
  "main": "index.js",
  "scripts": {
    "test": "c8 mocha --exit --timeout=8000 ./test/*.js",
    "start": "node index.js",
    "lint": "xo",
    "fix": "xo --fix",
    "migrate:create": "db-migrate create --config=./database.js",
    "migrate:up": "db-migrate up --config=./database.js",
    "migrate:down": "NODE_ENV=development db-migrate down --config=./database.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcoe/flaky.dev.git"
  },
  "keywords": [
    "flkay",
    "dev"
  ],
  "author": "Ben Coe <bencoe@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/bcoe/flaky.dev/issues"
  },
  "homepage": "https://github.com/bcoe/flaky.dev#readme",
  "dependencies": {
    "@octokit/rest": "^17.1.1",
    "body-parser": "^1.19.0",
    "connect-pg-simple": "^6.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "simple-oauth2": "^3.3.0",
    "uuid": "^7.0.2",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "c8": "^7.1.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "xo": "^0.28.0"
  },
  "xo": {
    "extends": "./.eslintrc.yml"
  }
}
